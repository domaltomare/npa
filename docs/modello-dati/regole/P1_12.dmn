        <inputEntry id="UnaryTests_P1_12_0117">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_P1_12_0039">
          <text>"{\"idTipologica\":\"errori\",\"codice\":{\"REG67\"}}"</text>
        </outputEntry>
      </rule>
    </decisionTable>
